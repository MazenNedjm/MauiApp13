<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
              xmlns:mct="clr-namespace:CommunityToolkit.Maui.Views;assembly=CommunityToolkit.Maui"
             
             x:Class="MauiApp13.pages.fourniss.Addd1">

    <ScrollView>
        <FlexLayout BindableLayout.ItemsSource="{Binding People}" Wrap="Wrap"
                    AlignContent="Start"
                    AlignItems="Start"
                    
                    >
            <BindableLayout.ItemTemplate>
                <DataTemplate>
                    <Frame WidthRequest="200" HeightRequest="200" CornerRadius="20"
                           Margin="0,10,10,0"
                           Background="#4b4747" BorderColor="Transparent" >
                        <Label Text="{Binding Name}" Margin="10"  BackgroundColor="Transparent"
                           />
                    </Frame>
                   
                </DataTemplate>
            </BindableLayout.ItemTemplate>
        </FlexLayout>
    </ScrollView>
</ContentView>
